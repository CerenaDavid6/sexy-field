{
  "name": "tardigrades/sexy-field",
  "description": "SexyField helps you build application structure and accompanying forms.",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Dion Snoeijen",
      "email": "hallo@dionsnoeijen.nl"
    }
  ],
  "require": {
    "php": "^7.1",
    "doctrine/orm": "^2.5.6",
    "symfony/console": "^4",
    "symfony/form": "^4",
    "symfony/validator": "^4",
    "symfony/security": "^4",
    "symfony/dependency-injection": "^4",
    "symfony/inflector": "^4",
    "symfony/yaml": "^4",
    "symfony/config": "^4",
    "symfony/event-dispatcher": "^4",
    "beberlei/assert": "2.6.5",
    "twig/twig": "^2.0",
    "twig/extensions": "^1.5.1",
    "php-coveralls/php-coveralls": "^1.0",
    "symfony/cache": "^4"
  },
  "autoload": {
    "psr-4": {
      "Tardigrades\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tardigrades\\": "test/unit/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1",
    "mockery/mockery": "^0.9.9",
    "mikey179/vfsstream": "^1.6",
    "phing/phing": "^2.16",
    "squizlabs/php_codesniffer": "^3.2"
  }
}
